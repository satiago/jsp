<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                         http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">
	<jsp-config>
	    <taglib>
	        <taglib-uri>http://www.test.com/mytag</taglib-uri>
	        <taglib-location>/WEB-INF/mytag.tld</taglib-location>
	    </taglib>
	</jsp-config>
	<filter>
		<filter-name>EncodingFilter</filter-name>
		<filter-class>com.EncodingFilter</filter-class>
		<init-param>
			<param-name>encoding</param-name>
			<param-value>UTF-8</param-value>
		</init-param>
	</filter>
	<filter-mapping>
		<filter-name>EncodingFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
	<listener>
	    <listener-class>com.CounterListener</listener-class>
	</listener>
	<servlet>
	    <servlet-name>UserServlet</servlet-name>
	    <servlet-class>com.UserServlet</servlet-class>
	</servlet>
	<servlet-mapping>
	    <servlet-name>UserServlet</servlet-name>
	    <url-pattern>/UserServlet.do</url-pattern>
	</servlet-mapping>
</web-app>